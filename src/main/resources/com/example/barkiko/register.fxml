         <Button fx:id="buttonAtras" layoutX="16.0" layoutY="315.0" mnemonicParsing="false" onAction="#goAtras" prefHeight="32.0" prefWidth="141.0" stylesheets="@../../../recursos/css.css" text="Atrás" textAlignment="CENTER">
