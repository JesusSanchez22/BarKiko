<VBox id="fondo" alignment="CENTER" prefHeight="464.0" prefWidth="508.0" spacing="20.0" stylesheets="@../../../recursos/css.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.barkiko.RegisterController">
